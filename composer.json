{
  "name": "bluemvc/bluemvc-api",
  "description": "API extension for the BlueMvc PHP framework",
  "homepage": "https://bluemvc.com/",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "blue",
    "mvc",
    "framework",
    "php",
    "api"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "datatypes/datatypes": "^1.0||^2.0",
    "bluemvc/bluemvc-core": "^2.2@dev"
  },
  "require-dev": {
    "bluemvc/bluemvc-fakes": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "BlueMvc\\Api\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlueMvc\\Api\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.2-dev"
    }
  }
}
